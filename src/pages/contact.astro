          <!-- Embedded booking begin -->
          <div
            class="embedded-booking"
            data-url="https://ruletheweb.trafft.com"
            data-autoresize="1"
            data-showsidebar="1"
            style="min-width: 320px; height: 768px;"
          >
          </div>
          <script
            type="text/javascript"
            src="https://ruletheweb.trafft.com/embed.js"
            async></script>
          <!-- Embedded booking end -->
        </div>
        <!-- Modal footer -->
        <div
          class="flex items-center p-4 md:p-5 border-t border-gray-200 rounded-b dark:border-gray-600"
        >
        </div>
      </div>
    </div>
